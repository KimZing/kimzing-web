  "scripts": {
    "#运行": "以不同模式本地运行，其中mock自带数据",
    "mock": "vite --mode mock",
    "dev": "vite --mode development",
    "perf": "vite --mode performance",
    "uat": "vite --mode useraccept",
    "pro": "vite --mode production",
    "#构建": "以不同模式构建，其中mock自带数据",
    "build:mock": "vite build --mode mock",
    "build:dev": "vite build --mode development",
    "build:perf": "vite build --mode performance",
    "build:uat": "vite build --mode useraccept",
    "build:pro": "vite build --mode production",
    "#类型检查": "检查类型",
    "check:type": "vue-tsc --noEmit --skipLibCheck",
    "#预览": "以不同模式预览生成的dist包",
    "preview:mock": "vite build --mode mock && vite preview --port 5000",
    "preview:dev": "vite build --mode development && vite preview --port 5001",
    "preview:perf": "vite build --mode performance && vite preview --port 5002",
    "preview:uat": "vite build --mode useraccept && vite preview --port 5003",
    "preview:pro": "vite build --mode production && vite preview --port 5004",
    "#检查及格式化": "检查代码格式并修正",
    "lint": "eslint --cache --max-warnings 0  \"src/**/*.{vue,ts,tsx}\" --fix"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "crypto-js": "^4.1.1",
