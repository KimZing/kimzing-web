    "vue-router": "^4.0.11",
    "vuex": "^4.0.2",
    "vuex-persistedstate": "^4.0.0",
    "xlsx": "^0.17.1"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.0.2",
    "@types/file-saver": "^2.0.3",
    "@types/mockjs": "^1.0.4",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
