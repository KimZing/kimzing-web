{
  "name": "vite3-vue3-antd-ts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "#运行": "运行不同模式",
    "dev": "vite serve --mode development",
    "pro": "vite serve --mode production",
    "#构建": "打包不同模式",
    "build:dev": "vue-tsc && vite build --mode development",
    "build:pro": "vue-tsc && vite build --mode production",
    "#预览": "预览不同模式",
    "preview:dev": "vite build --mode development && vite preview",
    "preview:pro": "vite build --mode production && vite preview",
    "#检查": "检查修复代码",
    "lint": "eslint --fix \"src/**/*.{vue,ts,tsx}\" "
  },
  "dependencies": {
    "ant-design-vue": "^3.2.15",
    "axios": "1.2.0",
    "mockjs": "1.1.0",
    "vue": "^3.2.41",
    "vue-router": "4",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@types/mockjs": "^1.0.7",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vitejs/plugin-vue": "^3.2.0",
    "eslint": "^8.28.0",
    "eslint-plugin-vue": "^9.8.0",
    "less": "^4.1.3",
    "typescript": "^4.9.3",
    "vite": "^3.2.3",
    "vue-tsc": "^1.0.9"
  }
}
